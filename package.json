{
  "name": "ssr-rental-cars",
  "version": "1.0.0",
  "description": "react SSR app based on rentalcars.com",
  "main": "server/index.js",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:server": "nodemon --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "test": "jest --watch --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tornado1979/ssr-rentalcars.git"
  },
  "keywords": [
    "reactjs",
    "redux",
    "ssr",
    "api",
    "rentalcars"
  ],
  "author": "Theodoros Itzaris",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tornado1979/ssr-rentalcars/issues"
  },
  "homepage": "https://github.com/tornado1979/ssr-rentalcars#readme",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@webpack-cli/init": "^0.1.2",
    "autoprefixer": "^9.3.1",
    "axios": "0.16.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-react": "^6.24.1",
    "compression": "^1.7.3",
    "concurrently": "3.5.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.7.0-alpha.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.7.0-alpha.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.1",
    "reselect": "^4.0.0",
    "serialize-javascript": "^1.5.0",
    "url-loader": "^1.1.2",
    "webpack-node-externals": "1.6.0",
    "youtube-api-search": "0.0.5"
  },
  "devDependencies": {
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "jest": "^23.6.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.10.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "rootDir": ".",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(scss|css|less)$": "identity-obj-proxy",
      "\\.(gif|png|jpg|jpeg|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.css$": "custom-transformer",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
